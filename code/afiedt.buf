CREATE DIMENSION TARGET_DW.DATE_DIM
LEVEL L1_YEAR IS DATE_DIM.YEAR_QUARTER_NUM
LEVEL L2_QUARTER IS DATE_DIM.QUARTER_NUM
LEVEL L3_MONTH IS DATE_DIM.MONTH_NUM
LEVEL L4_WEEK IS DATE_DIM.WEEK_YEAR_NUM
LEVEL L5_DATE IS DATE_DIM.DATE_SMART_KEY
HIERARCHY H1_STANDARD1 (L5_DATE CHILD  OF L3_MONTH CHILD OF L2_QUARTER CHILD OF L1_YEAR)
HIERARCHY H1_STANDARD2 (L5_DATE CHILD OF L4_WEEK  CHILD OF L1_YEAR)
/
